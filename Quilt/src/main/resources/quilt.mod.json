{
  "schema_version": 1,
  "quilt_loader": {
    "group": "foundry",
    "id": "veil",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "metadata": {
      "name": "Veil",
      "description": "This is an example description! Tell everyone what your mod is about!",
      "authors": [
        "Me!"
      ],
      "contact": {
        "homepage": "https://quiltmc.org/",
        "sources": "https://github.com/Oliver-makes-code/quilt-example-mod"
      },
      "license": "CC0-1.0",
      "icon": "assets/modid/icon.png"
    },
    "entrypoints": {
      "init": "foundry.veil.VeilQuilt",
      "client_init": "foundry.veil.VeilQuiltClient"
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": "1.19.2"
      },
      {
        "id": "java",
        "version": ">=17"
      }
    ]
  },
  "mixin": [
    "veil-common.mixins.json",
    "veil-quilt.mixins.json"
  ],
  "accessWidener": "veil.accesswidener"
}