{
  "schemaVersion": 1,
  "id": "veil",
  "version": "${version}",
  "name": "Veil",
  "description": "This is the Veil project. It is a collection of tools and utilities that are designed to make it easier for mod developers to do advanced rendering in their mods.",
  "authors": [
    "amo",
    "Cappin",
    "Ocelot"
  ],
  "contact": {
    "issues": "https://github.com/FoundryMC/Veil/issues",
    "sources": "https://github.com/FoundryMC/Veil"
  },
  "license": "All Rights Reserved",
  "environment": "*",
  "entrypoints": {
    "main": [
      "foundry.veil.fabric.VeilFabric"
    ],
    "client": [
      "foundry.veil.fabric.VeilFabricClient"
    ]
  },
  "mixins": [
    "veil-common.mixins.json",
    "veil-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric": "*",
    "minecraft": "1.20.x",
    "java": ">=17"
  },
  "accessWidener": "veil.accesswidener"
}
  